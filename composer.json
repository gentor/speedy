  "type": "library",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Lyubomir Gardev",
      "email": "rolice@intellisys.info"
    },
    {
      "name": "Evgeni Nurkov",
      "email": "evgeni.nurkov@gmail.com"
    },
    {
      "name": "Georgi Nachev",
      "email": "jooorooo@gmail.com"
    }
